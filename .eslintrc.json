{
	"env": {
	  "browser": true,
	  "es6": true,
	  "node": true
	},
	"extends": [
	  "eslint:recommended",
	  "prettier"
	],
	"plugins": [
	  "prettier"
	],
	"parserOptions": {
	  "ecmaVersion": 2020
	},
	"rules": {
	  "prettier/prettier": "error",
	  "camelcase": [
		"error"
	  ],
	  "ignoreShorthands:":["/padding/", "/border/","flex"],
	  "selector-class-pattern": null,
	  "strict": "error",
	  "resolveNestedSelectors": true,
	  "eqeqeq": [
		"error",
		"always"
	  ]
	}
  }